package TazwarBelal;

import java.awt.*;

public abstract class RPentagon extends Shape implements RPentagon1 {
    private int side;

    public RPentagon(int side)
    {
        this.side = side;
    }
@Override
    public abstract double getArea()
    {
        return 0.25*Math.sqrt(5*(5+2*Math.sqrt(5)))*(side*side);
    }
    @Override
    public double getPerimeter()
    {
        return side*5;
    }

}
